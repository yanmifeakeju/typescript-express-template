{
  "name": "typescript-express-template",
  "version": "1.0.0",
  "description": "Base Template For Starting Express API",
  "main": "dist/index.js",
  "engines": {
    "node": "16.*",
    "npm": "8.*"
  },
  "scripts": {
    "lint": "eslint src",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "postinstall": "npm run build",
    "prepare": "husky install",
    "build": "tsc",
    "start": "npm run build && node -r dotenv/config dist/index.js",
    "start:dev": "nodemon -r dotenv/config src/index.ts",
    "start:test": "dotenv -e .env.test -- nodemon dist/index.js",
    "test:e2e": "dotenv -e .env.test ./scripts/e2e.test.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prisma:migrate-dev": "prisma migrate dev --schema=src/infrastructure/database/prisma/schema.prisma",
    "prisma:db-push": "prisma db push --schema=src/infrastructure/database/prisma/schema.prisma",
    "prisma:migrate-deploy": "prisma migrate deploy --schema=src/infrastructure/database/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=src/infrastructure/database/prisma/schema.prisma",
    "migrate:postgres:test": "dotenv -e .env.test -- npm run prisma:migrate-dev --name postgres-init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanmifeakeju/typescript-express-template.git"
  },
  "keywords": [],
  "author": "Oluwayanmife Akeju",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yanmifeakeju/typescript-express-template/issues"
  },
  "homepage": "https://github.com/yanmifeakeju/typescript-express-template#readme",
  "devDependencies": {
    "@cucumber/cucumber": "^8.4.0",
    "@types/chance": "^1.1.3",
    "@types/convict": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@types/superagent": "^4.1.15",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "chance": "^1.1.8",
    "eslint": "^8.17.0",
    "husky": "^8.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "progress-bar": "^0.1.1",
    "superagent": "^8.0.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "convict": "^6.2.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.3.8",
    "winston": "^3.7.2"
  },
  "volta": {
    "node": "16.16.0",
    "npm": "8.13.2"
  }
}
